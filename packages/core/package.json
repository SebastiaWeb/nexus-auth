{
  "name": "@nexusauth/core",
  "version": "0.1.1",
  "description": "Core authentication library for NexusAuth - Framework agnostic, TypeScript-first authentication with schema mapping",
  "keywords": [
    "auth",
    "authentication",
    "jwt",
    "oauth",
    "oauth2",
    "session",
    "passport",
    "nextauth",
    "typescript",
    "security"
  ],
  "author": "NexusAuth Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastiaWeb/nexus-auth.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/SebastiaWeb/nexus-auth#readme",
  "bugs": {
    "url": "https://github.com/SebastiaWeb/nexus-auth/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.0",
    "bcryptjs": "^3.0.2"
  }
}
